{
  "expo": {
    "name": "CarBnb",
    "slug": "CarBnb",
    "version": "1.0.0",
    "splash": {
      "image": "./assets/images/Logo-negro.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "orientation": "default",
    "icon": "./assets/images/Logo-negro.jpg",
    "scheme": "carbnb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carbnb.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/LogosinletrasB.png",
        "backgroundImage": "./assets/images/LogosinletrasB.png",
        "monochromeImage": "./assets/images/LogosinletrasB.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.carbnb.app",
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo-negro.jpg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
